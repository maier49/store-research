	"files": [
		"./typings/index.d.ts",
		"./src/patch/jsonPointer.ts",
		"./src/patch/operation.ts",
		"./src/patch/patch.ts",
		"./src/store/Filter.ts",
		"./src/store/Query.ts",
		"./src/store/Range.ts",
		"./src/store/Sort.ts",
		"./src/store/Store.ts",
		"./src/utils.ts",
		"./tests/intern-local.ts",
		"./tests/intern.ts",
		"./tests/unit/all.ts",
		"./tests/unit/store/filter.ts"
	]
}
