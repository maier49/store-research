{
	"version": "1.8.10",
	"compilerOptions": {
		"declaration": false,
		"module": "umd",
		"noImplicitAny": true,
		"outDir": "_build/",
		"removeComments": false,
		"sourceMap": true,
		"target": "es5",
		"moduleResolution": "classic"
	},
	"filesGlob": [
		"./typings/index.d.ts",
